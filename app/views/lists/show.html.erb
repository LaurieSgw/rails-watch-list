<h1>Lists#show</h1>

<%= @list.name %>

<%= link_to "New bookmark", new_list_bookmark_path(@list) %>

<ul>
<% @list.bookmarks.each do |bookmark| %>
 <li> <%= bookmark.movie.title %>
  <%= bookmark.movie.overview %>
  <%= bookmark.movie.poster_url %>
  <%= bookmark.movie.rating%>
  <%= bookmark.comment %>
  <%= link_to "Delete", list_bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %></li>
<% end %>
</ul>
